{
  "family": "simple-ecr-docker-github-actions",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "simple-ecr-docker-github-actions",
      "image": "618342723525.dkr.ecr.us-west-2.amazonaws.com/simple-ecr-docker-github-actions",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "256",
  "memory": "512"
}